<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="660.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewController.LoginScreenController">
   <children>
      <Label fx:id="passwordLabel" layoutX="204.0" layoutY="450.0" text="Password:">
         <font>
            <Font name="Arial" size="16.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" layoutX="204.0" layoutY="400.0" text="Username:">
         <font>
            <Font name="Arial" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="341.0" layoutY="400.0" prefHeight="27.0" prefWidth="150.0" />
      <TextField fx:id="passwordTextField" layoutX="341.0" layoutY="450.0" prefHeight="27.0" prefWidth="150.0" />
      <Button fx:id="exitButton" layoutX="573.0" layoutY="547.0" mnemonicParsing="false" onAction="#exitButtonHandler" prefHeight="34.0" prefWidth="120.0" text="Exit" />
      <Button fx:id="loginButton" layoutX="362.0" layoutY="547.0" mnemonicParsing="false" onAction="#loginButtonHandler" prefHeight="34.0" prefWidth="120.0" text="Login" />
      <Label fx:id="intro" layoutX="323.0" layoutY="191.0" text="Please Login">
         <font>
            <Font name="Arial" size="16.0" />
         </font>
      </Label>
      <Label fx:id="title" alignment="CENTER" layoutX="296.0" layoutY="108.0" text="WELCOME">
         <font>
            <Font name="Zapfino" size="36.0" />
         </font>
      </Label>
      <Label fx:id="loginHintLabel" layoutX="200.0" layoutY="298.0" />
      <Label fx:id="errorMessageLabel" layoutX="200.0" layoutY="330.0" />
   </children>
</AnchorPane>
